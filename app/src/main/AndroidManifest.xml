<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.safechat">

    <application
        android:name=".SecureMessengerApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".registration_view.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".conversation.select.SelectConversationActivity"/>
        <activity android:name=".conversation.symmetrickey.ExchangeSymmetricKeyActivity"/>
    </application>
</manifest>
